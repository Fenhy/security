<hibernate-mapping>
  <sql-query name="getUserByUnameAndPwd">
    <![CDATA[
      SELECT userid,
      		 username,
             password
      FROM user
      WHERE username = :username
      AND password = :password
    ]]>
  </sql-query>
</hibernate-mapping>
